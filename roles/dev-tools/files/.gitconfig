[user]
        name = Jim Ladd
        email = jladd@redhat.com
[push]
        default = current
[alias]
        co = checkout
        st = status
        br = branch
        r = remote -v
        sbr = branch --sort=-committerdate -a
        last = log -1 HEAD
        llog = log --pretty='%<(11)%h %<(18)%an %s'
[color]
        diff = auto
        status = auto
        branch = auto
        interactive = auto
        ui = true
        pager = true
[rebase]
        instructionFormat = (%an <%ae>) %s
